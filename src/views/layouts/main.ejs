                    <li>
                      <span class="settings-option">Incognito mode</span><input type="checkbox" class="js-switch" />
                    </li>
                    <li>
                      <span class="settings-option">Public profile</span><input type="checkbox" class="js-switch" />
                    </li>
                  </ul>
